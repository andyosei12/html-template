{
  "name": "nana-reagan",
  "version": "1.0.0",
  "description": "Html template for Nana Reagan Blog Site",
  "browserslist": "last 10 versions",
  "main": "index.js",
  "scripts": {
    "devserver": "lite-server",
    "watch:sass": "node-sass sass/main.scss css/styles.css -w",
    "dev": "npm-run-all --parallel compile:sass devserver",
    "compile:sass": "node-sass sass/main.scss css/style.comp.css",
    "concat:css": "concat -o css/style.concat.css css/style.comp.css",
    "prefix:css": "postcss css/style.concat.css --use autoprefixer -o css/style.prefix.css",
    "compress:css": "node-sass css/style.prefix.css css/styles.css --output-style compressed",
    "build:css": "npm-run-all compile:sass concat:css prefix:css compress:css"
  },
  "author": "Drew",
  "license": "ISC",
  "dependencies": {
    "autoprefixer": "^10.4.2",
    "concat": "^1.0.3",
    "lite-server": "^2.6.1",
    "node-sass": "^7.0.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.6",
    "postcss-cli": "^9.1.0"
  }
}
